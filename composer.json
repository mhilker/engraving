{
    "name": "engraving/engraving",
    "description": "Engraving",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maik Hilker",
            "email": "maikhilker89@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "require": {
        "php": "7.4.*",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "suggest": {
        "http-interop/response-sender": "To use the HttpInteropResponseEmitter",
        "league/plates": "To use the PlatesRenderer",
        "nikic/fast-route": "To use the FastRouteRouter"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "http-interop/response-sender": "^1.0",
        "league/plates": "^3.3",
        "nikic/fast-route": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Engraving\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Engraving\\": "tests/"
        }
    }
}
